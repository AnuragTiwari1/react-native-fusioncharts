{
  "name": "react-native-fusioncharts",
  "version": "1.0.0",
  "description": "React Native component for FusionCharts Javascript Charting Library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fusioncharts/react-native-fusioncharts"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --coverage --verbose",
    "tplcompile": "node src/webview/template-compiler.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "npm run tplcompile && react-native run-android --variant=release",
    "ios:release": "npm run tplcompile && react-native run-ios --configuration Release",
    "build:android": "npm run tplcompile && cd android && ./gradlew assembleRelease && cd ..",
    "build:ios": "npm run tplcompile && react-native run-ios --configuration Release"
  },
  "author": {
    "name": "FusionCharts",
    "email": "support@fusioncharts.com"
  },
  "contributors": [
    {
      "name": "Rousan Ali",
      "email": "rousan@fusioncharts.com",
      "url": "https://github.com/ariyankhan"
    },
    {
      "name": "Subrata Mal",
      "email": "subrata@fusioncharts.com",
      "url": "https://github.com/subratamal"
    }
  ],
  "main": "src/FusionCharts.js",
  "bugs": {
    "url": "https://github.com/fusioncharts/react-native-fusioncharts/issues"
  },
  "keywords": [
    "android",
    "ios",
    "fusioncharts",
    "wrapper",
    "react",
    "react-native",
    "chart"
  ],
  "dependencies": {
    "fusioncharts": "^3.12.2"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "ejs": "^2.5.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "21.2.1",
    "react": "16.0.0",
    "react-native": "0.51.0",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
